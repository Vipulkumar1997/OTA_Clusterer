l)\r\n           hImg.src = img.src;\r\n        else\r\n           hImg.src = newSrc;\r\n\r\n        hImg.alt = img.alt;\r\n        //if(Sys.Browser.agent == Sys.Browser.InternetExplorer) {\r\n        //    if( !isNaN(img.fullWidth)) hImg.width = img.fullWidth; else hImg.width = undefined;\r\n        //    if( !isNaN(img.fullHeight)) hImg.height = img.fullHeight;  else hImg.height = undefined;\r\n        //}    \r\n    }\r\n\r\n    underscore = {\r\n        position: function(element) {\r\n            if (element.x && element.y) {\r\n                var p = { x: 0, y: 0 };\r\n                p.x = element.x + element.width;\r\n                p.y = element.y + element.height;\r\n            }\r\n            else {\r\n                var p = { x: element.offsetLeft || 0, y: element.offsetTop || 0 };\r\n                p.x += element.offsetWidth;\r\n                p.y += element.offsetHeight;\r\n                while (element = element.offsetParent) {\r\n                    p.x += element.offsetLeft;\r\n                    p.y += element.offsetTop;\r\n                }\r\n            }\r\n            return p;\r\n        }\r\n    }\r\n\r\n    var floatImg = null;\r\n    function showFloatImage(img) {\r\n        if (img == null)\r\n            return;\r\n        var p = underscore.position(img);\r\n        floatImg = document.createElement('img');\r\n        floatImg.alt = \"Loading image ...\";\r\n        floatImg.title = \"Loading image ...\";\r\n        var newSrc = null;\r\n        if(img.src.indexOf('_thumb.gif') > 0)\r\n            newSrc = img.src.replace('_thumb.gif', '.jpg');\r\n        else\r\n        {\r\n            var regex = new RegExp('.+url=(\\\\w+)');\r\n            var match = regex.exec(img.src);\r\n            if(match != null)\r\n            {\r\n                var imgSrc = Base64.decode(match[1]);\r\n                if(imgSrc.indexOf('_thumb.gif') > 0)\r\n                    //newSrc = '/HotelsOne/image.aspx?url=' + Base64.encode(imgSrc.replace('_thumb.gif', '.jpg'));\r\n                    newSrc = imgSrc.replace('_thumb.gif', '.jpg');\r\n            }\r\n        }\r\n        if(newSrc == null)\r\n           floatImg.src = img.src;\r\n        else\r\n           floatImg.src = newSrc;\r\n        //floatImg.alt = img.alt;\r\n        //if(Sys.Browser.agent == Sys.Browser.InternetExplorer) {\r\n        //    if( !isNaN(img.fullWidth)) floatImg.width = img.fullWidth; else floatImg.width = undefined;\r\n        //    if( !isNaN(img.fullHeight)) floatImg.height = img.fullHeight;  else floatImg.height = undefined;       \r\n        //}\r\n        floatImg.style.position = 'absolute';\r\n        floatImg.style.left = p.x + 'px';\r\n        floatImg.style.top = p.y + 'px';\r\n        floatImg.className = 'floatImg';\r\n        document.body.appendChild(floatImg);\r\n    }\r\n    \r\n    function hideFloatImage(img) {\r\n        if (floatImg == null)\r\n            return;\r\n        document.body.removeChild(floatImg);\r\n        floatImg = null;\r\n    }\r\n    \r\n    \r\n    function mapStart() {\r\n        // creating map\r\n        var coordinates = new google.maps.LatLng('56.329917', '44.009192');\r\n        if( coordinates == null || (coordinates.lat() == 0 && coordinates.lng() == 0))\r\n        {\r\n            // getting coordinates\r\n            var hotelAddress = \"Gagarina avenue, 27 603057 Niznij Novgorod, Russia\";\r\n            var geocoder = new google.maps.Geocoder();\r\n            geocoder.geocode({ address: hotelAddress }, function(results, status) {\r\n                if (status == google.maps.GeocoderStatus.OK) {\r\n                    var mapOptions = {\r\n                        zoom: 14,\r\n                        center: results[0].geometry.location,\r\n                        mapTypeId: google.maps.MapTypeId.ROADMAP\r\n                    };\r\n                    var hotelMap = new google.maps.Map(document.getElementById(\"hotelMap\"), mapOptions);\r\n\r\n                    // creati